{
  "name": "flatgeobuf",
  "version": "3.1.0",
  "description": "A performant binary encoding for geographic data",
  "scripts": {
    "build": "tsc -p config/tsconfig.json && rollup -c config/rollup.config.js",
    "pretest": "ws &",
    "test": "TS_NODE_PROJECT=config/tsconfig.test.json mocha -r ts-node/register -r esm -t 20000 src/**/*.spec.ts",
    "typedoc": "typedoc --options config/typedoc.json src/ts"
  },
  "files": [
    "lib/**/*",
    "dist/**/*"
  ],
  "main": "lib/flatgeobuf.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bjornharrtell/flatgeobuf.git"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/bjornharrtell/flatgeobuf/issues"
  },
  "homepage": "https://github.com/bjornharrtell/flatgeobuf#readme",
  "dependencies": {
    "flatbuffers": "1.12.0",
    "slice-source": "0.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@rollup/plugin-node-resolve": "7.0.0",
    "@types/chai": "4.2.11",
    "@types/mocha": "7.0.2",
    "@types/node": "13.13.4",
    "chai": "4.2.0",
    "esm": "3.2.25",
    "jsts": "2.1.2",
    "local-web-server": "4.0.0",
    "mocha": "7.1.2",
    "node-fetch": "2.6.0",
    "ol": "6.3.1",
    "rollup": "2.7.6",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-terser": "5.3.0",
    "stream-buffers": "3.0.2",
    "ts-node": "8.9.1",
    "tslint": "6.1.2",
    "typedoc": "0.17.6",
    "typescript": "3.8.3",
    "web-streams-polyfill": "2.1.1"
  }
}

namespace FlatGeobuf;

enum GeometryType:ubyte {
  Point,
  MultiPoint,
  LineString,
  MultiLineString,
  Polygon,
  MultiPolygon
}

table Geometry {
  ring_counts: [uint]; // ring count per part in collection of Polygons
  ring_lengths: [uint]; // ring lengths
  lengths: [uint]; // coordinate lengths
  coords: [double] (required);
}

root_type Geometry;
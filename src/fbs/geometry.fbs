namespace FlatGeobuf;

enum GeometryType:ubyte {
  Point,
  MultiPoint,
  LineString,
  MultiLineString,
  Polygon,
  MultiPolygon,
  GeometryCollection
}

table Geometry {
  type: GeometryType;
  types: [GeometryType];
  dimensions: ubyte = 2;
  envelope: [double];
  ends: [uint];
  ring_ends: [uint];
  coords: [double];
}

root_type Geometry;
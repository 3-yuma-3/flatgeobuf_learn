namespace FlatGeobuf;

enum GeometryType:ubyte {
  Point,
  MultiPoint,
  LineString,
  MultiLineString,
  Polygon,
  MultiPolygon,
  GeometryCollection
}

table Geometry {
  type: GeometryType;
  envelope: [double];
  dimensions: ubyte;
  geometries: [Geometry];
  coord_lengths: [uint];
  coords: [double];
}

root_type Geometry;
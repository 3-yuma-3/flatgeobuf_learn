table Feature {
  fid: ulong;
  ends: [uint]; // array of end index in flat coordinates per geometry part
  lengths: [uint]; // array of number of parts per multigeom (only relevant for multipolygon)
  xy: [double]; // flat x and y array (flat pairs)
  z: [double]; // flat z coordinate array
  m: [double]; // flat z coordinate array
  t: [double]; // flat z coordinate array
  properties: [ubyte]; // custom buffer, array of key/value pairs (key=ushort)
}

root_type Feature;

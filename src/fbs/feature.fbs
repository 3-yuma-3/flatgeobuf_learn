namespace FlatGeobuf;

table Feature {
  fid: ulong;
  ends: [uint]; // coordinate part or subpart ends
  endss: [uint]; // subpart ends (multipolygon rings)
  coords: [double];
  z: [double];
  m: [double];
  t: [double];
  properties: [ubyte]; // custom buffer, array of key/value pairs (key=ushort)
}

root_type Feature;
